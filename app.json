{
  "expo": {
    "name": "Concept",
    "slug": "UMI-Mobile-App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/concept_icon.png",
    "scheme": "Concept",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "entryPoint": "expo-router/entry",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ismailakhan.UMIApp"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.ismailakhan.UMIApp"
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-screen.png",
          "resizeMode": "cover",
          "backgroundColor": "#000000",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-web-browser",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6fd130f0-f61e-49e8-bcf7-5deeb9a8be19"
      }
    }
  }
}
